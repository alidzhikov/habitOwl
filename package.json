{
  "name": "@ngrx/platform",
  "version": "7.0.0-beta.0",
  "description": "monorepo for ngrx development",
  "scripts": {
    "precommit": "lint-staged",
    "build": "bazel build ...",
    "deploy:builds": "ts-node ./build/deploy-build.ts",
    "test:unit": "node ./tests.js",
    "test": "nyc yarn run test:unit",
    "clean": "git clean -xdf",
    "cli": "ng",
    "coverage:html": "nyc report --reporter=html",
    "copy:dist": "ncp dist/ ./node_modules/@ngrx/",
    "example:start": "yarn run cli serve",
    "example:start:aot": "yarn run cli serve --prod",
    "example:test": "jest -c projects/example-app/jest.config.js --watch",
    "example:build:prod": "yarn cli build --prod --base-href \"/platform/example-app/\"",
    "ci": "yarn run test && nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --write \"**/*.ts\"",
    "watch:tests": "chokidar 'modules/**/*.ts' --initial -c 'yarn run test:unit'",
    "postinstall": "opencollective postinstall",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "preskylint": "bazel build --noshow_progress @io_bazel//src/tools/skylark/java/com/google/devtools/skylark/skylint:Skylint",
    "skylint": "find . -type f -name \"*.bzl\" ! -path \"*/node_modules/*\" ! -path \"./dist/*\" | xargs $(bazel info bazel-bin)/external/io_bazel/src/tools/skylark/java/com/google/devtools/skylark/skylint/Skylint --disable-checks=deprecated-api",
    "prebuildifier": "bazel build --noshow_progress @com_github_bazelbuild_buildtools//buildifier",
    "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier",
    "copy:schematics": "ts-node ./build/copy-schematics-core.ts",
    "build:stackblitz": "ts-node ./build/stackblitz.ts && git add ./stackblitz.html",
    "start-db": "cd ../HabitOwlApi && npm start",
    "start": "concurrently \"npm run start-db\" \"ng serve --open\""
  },
  "engines": {
    "node": ">=10.9.0 <11.2.0",
    "npm": ">=5.3.0",
    "yarn": ">=1.9.2 <2.0.0"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "ngrx",
    "angular",
    "rxjs"
  ],
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrx/platform.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*",
      "/modules/schematics/src/*/files/**/*",
      "/modules/**/schematics/**/files/**/*",
      "**/schematics/src/utility/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/bazel": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/platform-server": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@ngrx/db": "^2.2.0-beta.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/schematics": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "opencollective": "^1.0.3",
    "rxjs": "^6.5.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.0",
    "@ngrx/store-devtools": "^6.1.2",
    "@types/fs-extra": "^2.1.0",
    "@types/glob": "^5.0.33",
    "@types/globby": "^8.0.0",
    "@types/jasmine": "2.5.45",
    "@types/jasminewd2": "^2.0.2",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.123",
    "@types/ncp": "^2.0.1",
    "@types/node": "^7.10.6",
    "@types/ora": "^1.3.5",
    "@types/rimraf": "^0.0.28",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.2",
    "codelyzer": "^4.2.0",
    "concurrently": "^4.1.0",
    "conventional-changelog": "^1.1.4",
    "conventional-changelog-cli": "^1.3.21",
    "coveralls": "^2.13.0",
    "cpy-cli": "^1.0.1",
    "deep-freeze": "^0.0.1",
    "deep-freeze-strict": "^1.1.1",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.5.2",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "~3.2.0",
    "jest": "^21.0.2",
    "jest-preset-angular": "^3.0.1",
    "jest-zone-patch": "^0.0.7",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.5",
    "ncp": "^2.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "nyc": "^10.1.2",
    "ora": "^1.3.0",
    "prettier": "^1.17.0",
    "protractor": "~5.1.0",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^3.4.4",
    "rimraf": "^2.6.3",
    "rollup": "^0.50.0",
    "sorcery": "^0.10.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.8.0",
    "tsickle": "^0.32.1",
    "tslib": "^1.7.1",
    "tslint": "^5.16.0",
    "tsutils": "2.20.0",
    "typescript": "~3.1.1",
    "uglify-js": "^3.5.10"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ngrx",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
